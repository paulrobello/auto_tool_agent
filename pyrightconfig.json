{
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/dotenv",
    "**/build",
    "**/sandbox",
    "**/tool_archive"
  ],
  "ignore": [],
  "defineConstant": {
    "DEBUG": true
  },
  "reportMissingImports": "error",
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.11",
  "executionEnvironments": [
    {
      "root": "src",
      "reportMissingImports": "warning"
    },
    {
      "root": "tests",
      "extraPaths": []
    }
  ]
}
